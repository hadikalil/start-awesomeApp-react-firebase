es":"85","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"44"},"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/index.js",[],["86","87"],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/serviceWorker.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/App.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/CreateLink.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/SearchLinks.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/LinkList.js",["88"],"// after we did the creatLink and we have the data at the fire base we need to get it to the linklist\nimport React from \"react\";\nimport { FirebaseContext } from \"../../firebase\";\nimport LinkItem from \"./LinkItem\";\n\nfunction LinkList(props) {\n  // first we need the firebase instins so we need to consume from FirebaseContext ----// we need the firebase instance in order to make a reqest to the database\n  const { firebase } = React.useContext(FirebaseContext);\n\n  // second after fitching the data with onSnapshot() ---> we need to display it --> so -->>\n  // so we need a state for our links to live in\n  const [links, setLinks] = React.useState([]); // this will be an empty array --> set by  handleSnapshot()\n\n  // making a request when linkList component mounts\n  React.useEffect(() => {\n    getLinks();\n  }, []);\n\n  function getLinks() {\n    //firebase.db().collection('links').get() *********get returns a promise  and we can await the promise\n    firebase.db().collection(\"links\").onSnapshot(handleSnapshot); //*********** .onSnapshot() method to get the data ==> it sets up an active listener - like the onAuthStateChanged() listener that we used at useAuth.js\n    // .onSnapshot() method it's going to listen for any changes with the collection // so when we add new link it's going to be immediately fetched and desplayd in LinkList\n  }\n\n  function handleSnapshot(snapshot) {\n    // .onSnapshot() we get a callback function where we can work with all of the documents on a given collection **> snapshot.docs is an array\n    const links = snapshot.docs.map((doc) => {\n      return { id: doc.id, ...doc.data() }; // id:doc.id, will add to the object the collection-link-id\n    });\n    setLinks(links);\n    \n\n  }\n\n  return (\n    <div>\n    { links.map((link, index) => (\n      <LinkItem\n        key={link.id}\n        showCount={true}\n        link={link}\n        index={index + 1}\n      /> \n    ))}\n    </div>\n  );\n}\n\nexport default LinkList;","/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/LinkDetail.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/Login.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/ForgotPassword.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Header.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/useFormValidation.js",["89"],"import React from \"react\";\n\nfunction useFormValidation(initialState, validate, authenticate) {\n    const [values, setValues] = React.useState(initialState)\n    const [errors, setErrors] = React.useState({})\n    const [isSubmitting, setSubmitting] = React.useState(false)\n\n    React.useEffect(() => {\n\n        if (isSubmitting) {\n            // to see if there is errors well pass the obj to Object.keys to turn it into array \n            const noErrors = Object.keys(errors).length === 0\n            if (noErrors) { // if no errors we want to authenticate the user \n                // console.log('authenticated', values);\n                authenticate();\n                // then we want to set set submitting to false \n                setSubmitting(false)\n            } else {\n                setSubmitting(false)\n            }\n        }\n    }, [errors])\n\n    function handleChange(event) {\n        event.persist();\n        setValues(previousValues => ({\n            ...previousValues,\n            [event.target.name]: event.target.value\n        }))\n\n    }\n\n    function handleBlur() {\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n    }\n\n    function handleSubmit(event) {\n        event.preventDefault()\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n        setSubmitting(true)\n            //console.log(\"log handleSubmit\", { values });\n    }\n\n    return { handleChange, handleBlur, handleSubmit, values, errors, isSubmitting }\n}\n\nexport default useFormValidation;","/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/validateLogin.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/index.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/firebase.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/config.js",[],["90","91"],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/useAuth.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/context.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/validateCreateLink.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/LinkItem.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/utils/index.js",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":17,"column":6,"nodeType":"98","endLine":17,"endColumn":8,"suggestions":"99"},{"ruleId":"96","severity":1,"message":"100","line":22,"column":8,"nodeType":"98","endLine":22,"endColumn":16,"suggestions":"101"},{"ruleId":"92","replacedBy":"102"},{"ruleI