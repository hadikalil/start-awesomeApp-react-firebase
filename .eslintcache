lues] = React.useState(initialState)\n    const [errors, setErrors] = React.useState({})\n    const [isSubmitting, setSubmitting] = React.useState(false)\n\n    React.useEffect(() => {\n\n        if (isSubmitting) {\n            // to see if there is errors well pass the obj to Object.keys to turn it into array \n            const noErrors = Object.keys(errors).length === 0\n            if (noErrors) { // if no errors we want to authenticate the user \n                // console.log('authenticated', values);\n                authenticate();\n                // then we want to set set submitting to false \n                setSubmitting(false)\n            } else {\n                setSubmitting(false)\n            }\n        }\n    }, [errors])\n\n    function handleChange(event) {\n        event.persist();\n        setValues(previousValues => ({\n            ...previousValues,\n            [event.target.name]: event.target.value\n        }))\n\n    }\n\n    function handleBlur() {\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n    }\n\n    function handleSubmit(event) {\n        event.preventDefault()\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n        setSubmitting(true)\n            //console.log(\"log handleSubmit\", { values });\n    }\n\n    return { handleChange, handleBlur, handleSubmit, values, errors, isSubmitting }\n}\n\nexport default useFormValidation;","/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/validateLogin.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/index.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/firebase.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/config.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/useAuth.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":