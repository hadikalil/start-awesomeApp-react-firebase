\n            }\n        }\n    }, [errors])\n\n    function handleChange(event) {\n        event.persist();\n        setValues(previousValues => ({\n            ...previousValues,\n            [event.target.name]: event.target.value\n        }))\n\n    }\n\n    function handleBlur() {\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n    }\n\n    function handleSubmit(event) {\n        event.preventDefault()\n        const validationErrors = validate(values)\n        setErrors(validationErrors)\n        setSubmitting(true)\n            //console.log(\"log handleSubmit\", { values });\n    }\n\n    return { handleChange, handleBlur, handleSubmit, values, errors, isSubmitting }\n}\n\nexport default useFormValidation;","/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/validateLogin.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/index.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/firebase.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/config.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/useAuth.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/firebase/context.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Auth/validateCreateLink.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/components/Link/LinkItem.js",[],"/home/hadi/Desktop/code/start/udmy awesome app/startAwesome/src/utils/index.js",[],{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","severity":1,"message":"94","line":47,"column":6,"nodeType":"95","endLine":47,"endColumn":13,"suggestions":"96"},{"ruleId":"93","severity":1,"message":"97","line":19,"column":6,"nodeType":"95","endLine":19,"endColumn":8,"suggestions":"98"},{"ruleId":"93","severity":1,"message":"99","line":22,"column":8,"nodeTy